    "tag": "unreleased"
  },
  "attributes": {
    "name": "Human immunodeficiency virus 1",
    "reference name": "HIV-1",
    "reference accession": "NC_001802.1",
    "experimental": true
  },
  "meta": {
    "source code": "https://github.com/nextstrain/nextclade_data",
