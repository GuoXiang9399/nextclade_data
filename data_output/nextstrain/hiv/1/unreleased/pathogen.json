{
  "alignmentParams": {
    "allowedMismatches": 8,
    "excessBandwidth": 50,
    "kmerDistance": 50,
    "kmerLength": 10,
    "minMatchLength": 40,
    "terminalBandwidth": 50,
    "windowSize": 30
  },
  "compatibility": {
    "cli": "3.0.0-alpha.0",
    "web": "3.0.0-alpha.0"
  },
  "files": {
    "changelog": "CHANGELOG.md",
    "examples": "sequences.fasta",
    "genomeAnnotation": "genome_annotation.gff3",
    "pathogenJson": "pathogen.json",
    "readme": "README.md",
    "reference": "reference.fasta",
    "treeJson": "tree.json"
  },
  "qc": {
    "frameShifts": {
      "enabled": true
    },
    "missingData": {
      "enabled": false,
      "missingDataThreshold": 1000,
      "scoreBias": 100
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 8
